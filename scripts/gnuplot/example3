set key bottom

set terminal svg dynamic
set grid
set output "dist/example3/graph.svg"
plot "data/example3.dat" title "example3.dat" pointsize 0.5, \
    {C1} * x + {C0}

set terminal latex
unset grid
set output "dist/example3/graph.tex"
plot "data/example3.dat" title "example3.dat" pointsize 0.5, \
    {C1} * x + {C0}

exit