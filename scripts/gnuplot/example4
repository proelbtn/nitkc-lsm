set key bottom

set terminal svg dynamic
set grid
set output "dist/example4/graph.svg"
plot "data/example4.dat" title "example4.dat" pointsize 0.5, \
    {C11} * x + {C10}, \
    {C22} * x ** 2 + {C21} * x + {C20}

set terminal latex
unset grid
set output "dist/example4/graph.tex"
plot "data/example4.dat" title "example4.dat" pointsize 0.5, \
    {C11} * x + {C10}, \
    {C22} * x ** 2 + {C21} * x + {C20}

exit